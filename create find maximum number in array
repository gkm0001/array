// 
 #include<iostream>
 #include<vector>
 #include<limits.h>
 using namespace std;
 
int main() {
    int arr[]= { 1,2,3,4,5,6,7,8,9};
    int size = 9;
    int maxi = INT_MAX;

    for ( int i=0;i<size ;i++){

        if(arr[i]<maxi){
          maxi = arr[i];
            
        }
    }
    cout << maxi<<endl;


}
 

