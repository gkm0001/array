// 
 #include<iostream>
 #include<vector>
 using namespace std;
 int find( int arr[] , int key ,int size ){
     int flag =1;
      for ( int i=0;i<size;i++){
          if ( key==arr[i]){
              flag = 0;
          }
      }
       if (flag ==0) cout <<"item present in the list"<<endl;
      else cout <<"item is not present in the list ";
 }


int main() {
   int arr[10] = { 1,2,3,4,5,6,67,7};
   int size = 8;
   
   cout <<"Enter the key to find"<<endl;
   int key;
    cin>>key;
   find(arr,key,size);
}
 

