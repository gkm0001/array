 
 #include<iostream>
 #include<vector>
 #include<limits.h>
 using namespace std;
 
int main() {
  int arr[8] = {10,30,30,40,50,60,30,80};
	int size = 8;

	int start = 0;
	int end =  size-1;

	while(start <= end) {
   swap ( arr[start],arr[end]);
       start ++;
     end--;
   }
     for ( int i=0;i< size;i++){
        cout << arr[i]<<" ";
     }


}
 

